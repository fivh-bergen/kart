    "lunr": "^2.3.9",
    "maplibre-gl": "^5.0.1",
    "nanostores": "^0.11.3",
    "opening_hours": "^3.8.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/lunr": "^2.3.7",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "osmtogeojson": "3.0.0-beta.5"
