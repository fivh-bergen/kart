  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "node --test \"**/*.test.js\"",
    "test:integration": "node --test \"**/*.spec.js\"",
    "test:snapshot": "node --test --test-update-snapshots \"**/*.test.js\"",
